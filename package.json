{
  "name": "arcadia",
  "version": "0.0.1",
  "description": "Light weight, modern, JavaScript utility belt",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "jshint src -c .jshintrc",
    "build": "node_modules/gulp/bin/gulp.js",
    "build-systemjs": "node_modules/gulp/bin/gulp.js --output systemjs",
    "build-commonjs": "node_modules/gulp/bin/gulp.js --output commonjs",
    "build-amd": "node_modules/gulp/bin/gulp.js --output amd",
    "production": "node_modules/gulp/bin/gulp.js --env production",
    "production-systemjs": "node_modules/gulp/bin/gulp.js --output systemjs --env production",
    "production-commonjs": "node_modules/gulp/bin/gulp.js --output commonjs --env production",
    "production-amd": "node_modules/gulp/bin/gulp.js --output amd --env production"
  },
  "license": "MIT",
  "repository": "tomsta93/arcadia",
  "author": {
    "name": "Thomas Stapleton",
    "url": "https://github.com/tomsta93/arcadia"
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-amd": "^6.3.13",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.3.16",
    "babel-plugin-transform-es2015-modules-systemjs": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "browserify": "^13.0.0",
    "del": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-concat": "^2.6.0",
    "gulp-debug": "^2.1.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.1",
    "gulp-util": "^3.0.7",
    "jshint": "^2.8.0",
    "run-sequence": "^1.1.5",
    "vinyl-source-stream": "^1.1.0"
  }
}
